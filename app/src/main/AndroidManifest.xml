<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Electrical"
            android:label="Electrical Engineering"
            android:theme="@style/ElectTheme"/>

        <activity android:name=".Civil"
            android:label="Civil Engineering"
            android:theme="@style/CivilTheme"/>

        <activity android:name=".Mechanical"
            android:label="Mechanical Engineering"
            android:theme="@style/MechTheme"/>

    </application>

</manifest>